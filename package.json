{
  "name": "cspace-ui",
  "version": "0.8.3",
  "description": "CollectionSpace user interface for browsers",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace",
    "ui",
    "user",
    "interface",
    "web",
    "browser"
  ],
  "main": "src/index.jsx",
  "files": [
    "dist",
    "lib",
    "src",
    "styles",
    "images"
  ],
  "repository": "collectionspace/cspace-ui.js",
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd",
    "build-cjs": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "build-umd": "npm run build-umd-dev && npm run build-umd-prod",
    "build-umd-dev": "webpack",
    "build-umd-prod": "cross-env NODE_ENV=production webpack",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf build lib dist coverage",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "devserver": "webpack-dev-server --inline --content-base . --history-api-fallback",
    "doc": "documentation readme src/*.js -s \"API\"",
    "lint": "eslint --ext .jsx,.js *.js src test examples",
    "prepublish": "npm run clean && npm run build",
    "start": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --singleRun=true"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cspace-client": ">=0.0.1 <0.1.0",
    "cspace-input": ">=0.9.5 <0.10.0",
    "cspace-layout": ">=0.1.13 <0.2.0",
    "cspace-refname": ">=0.0.3 <0.1.0",
    "deep-assign": "^2.0.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.16.4",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-image-gallery": "^0.7.12",
    "react-intl": "^2.1.5",
    "react-modal": "^1.7.3",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-virtualized": "^8.9.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "scriptjs": "^2.5.8",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.11.12",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "documentation": "^4.0.0-beta9",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "moxios": "^0.3.0",
    "nyc": "^8.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-shallow-testutils": "^2.0.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
