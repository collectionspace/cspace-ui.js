{
  "name": "cspace-ui",
  "version": "1.0.0-rc.62",
  "description": "CollectionSpace user interface for browsers",
  "author": "Ray Lee <rhlee@berkeley.edu>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cspace",
    "ui",
    "user",
    "interface",
    "web",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/collectionspace/cspace-ui.js.git"
  },
  "main": "src/index.jsx",
  "files": [
    "dist",
    "lib",
    "src",
    "styles",
    "images"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-umd",
    "build-cjs": "cross-env BABEL_ENV=cjs babel ./src -d lib && cross-env BABEL_ENV=cjs babel ./test/integration -d lib/test/integration",
    "build-umd": "npm run build-umd-dev && npm run build-umd-prod",
    "build-umd-dev": "webpack",
    "build-umd-prod": "cross-env NODE_ENV=production webpack",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf build lib dist coverage errorShots",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "devserver": "webpack-dev-server",
    "doc": "npx babel-node generateDocs.js",
    "lint": "eslint --ext .jsx,.js *.js src test examples",
    "prepare": "npm run clean && npm run build",
    "start": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --singleRun=true",
    "test-selenium": "wdio wdio.conf.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cspace-client": "^1.0.0",
    "cspace-input": "^1.0.10",
    "cspace-layout": "^1.0.0",
    "cspace-refname": "^1.0.0",
    "history": "4.6.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "openseadragon": "^2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.5.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.2.0",
    "react-image-gallery": "^0.8.10",
    "react-intl": "^2.4.0",
    "react-redux": "^4.4.5",
    "react-router": "4.1.2",
    "react-router-dom": "4.1.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-istanbul": "^1.0.3",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^3.0.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.28.10",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.2",
    "jsonfile": "^4.0.0",
    "karma": "^2.0.5",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "loglevel": "^1.6.1",
    "mocha": "^5.2.0",
    "moxios": "^0.3.0",
    "nyc": "^11.5.0",
    "react-shallow-testutils": "^3.0.1",
    "react-test-renderer": "^15.6.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "^0.5.12",
    "wdio-sauce-service": "^0.4.12",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
